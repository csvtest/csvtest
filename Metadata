#Catalog

< INSERIRE URI NOSTRA RACCOLTA >
	a dcatapit:Catalog , dcat:Catalog ;
	dct:title "Catalogo progetto INSERIRE NOME DEL PROGETTO"@it ;
	dct:description "Il catalogo dei dati aperti del progetto INSERIRE NOME DEL PROGETTO"@it ;
        dct:publisher <INSERIRE URI RISORSA CHE IDENTIFICA IL NOSTRO GRUPPO> ;
	dct:modified "2018-01-19"^^xsd:date ;
        dct:issued "2018-01-19"^^xsd:date ;
        dct:language <http://publications.europa.eu/resource/authority/language/ITA> ;
	foaf:homepage <INSERIRE URI HOMEPAGE> ;
        dcat:dataset <INSERIRE URI DATASET 1> ;
	dcat:dataset <INSERIRE URI DATASET 2> ;
	dcat:dataset <INSERIRE URI DATASET 3> ;
	dcat:themeTaxonomy <http://publications.europa.eu/resource/authority/data-theme> .


#Dataset 1


<INSERISCI URI RISORSA>
	a dcatapit:Dataset , dcat:Dataset ;
	dct:identifier "INSERISCI IDENTIFIER" ;
	dct:title "Analisi politiche 2018 Bologna"@it ;
	dct:description "Il dataset contiene i dati divisi per zona di Bologna di: redditi medi pro-capite dei contribuenti bolognesi dal 2009 al 2016, i risutati degli scrutini alle elezioni politiche per l'elezione al Senato della Repubblica e alla Camera dei Deputati del 4.3.2018, e il numero di residenti."@it ;
	dct:modified "2018-01-19"^^xsd:date ;
	dct:issued "2018-01-19"^^xsd:date ;
	owl:versionInfo "0.1.0" ; 
        dct:temporal <INSERIRE URI> ;
	dcat:theme <http://publications.europa.eu/resource/authority/data-theme/ECON> ;
	dcat:theme <http://publications.europa.eu/resource/authority/data-theme/SOCI> ; 
	dcat:theme <http://publications.europa.eu/resource/authority/data-theme/GOVE> ;
	dct:subject <http://eurovoc.europa.eu/700> , <http://eurovoc.europa.eu/3319> , <http://eurovoc.europa.eu/3460>; # elezioni nazionali,  distribuzione del reddito , residenza
	dcat:keyword "elezioni"@it , "bologna"@it, "redditi"@it ;
	dct:spatial <https://www.geonames.org/3181928/> ;
	dct:accrualPeriodicity <http://publications.europa.eu/resource/authority/frequency/OTHER> ; # other = ogni 5 anni
	dcat:distribution <INSERIRE URI/Distribuzione/BOLIDE-RDFTURTLE> ;
	dcat:distribution <INSERIRE URI/Distribuzione/BOLIDE-RDFTURTLE> ;
	dcat:distribution <INSERIRE URI/Distribuzione/BOLIDE-RDFTURTLE> ;
	dcat:contactPoint <INSERIRE URI PUNTO DI CONTATTO> ;
	dct:rightsHolder <INSERIRE URI RISORSA CHE IDENTIFICA IL NOSTRO GRUPPO> ;
	dct:publisher <INSERIRE URI PUBLISHER> ;
	dct:creator <INSERIRE URI CREATOR> ;
	dct:language <http://publications.europa.eu/resource/authority/language/ITA> ;
	dcat:landingPage <INSERIRE URL ACCESSO AL DATASET> ;
	prov:wasDerivedFrom <http://dati.comune.bologna.it/node/244> ; #redditi
	prov:wasDerivedFrom <http://dati.comune.bologna.it/node/3328> ; #politiche camera
	prov:wasDerivedFrom <http://dati.comune.bologna.it/node/3323> ; #politiche senato
	prov:wasDerivedFrom <http://dati.comune.bologna.it/node/161> ; #aree statistiche in zone

